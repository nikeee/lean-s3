{
  "name": "lean-s3",
  "author": "Niklas Mollenhauer",
  "license": "MIT",
  "version": "0.3.2",
  "description": "A server-side S3 API for the regular user.",
  "keywords": [
    "s3",
    "client",
    "s3 client",
    "s3 sdk",
    "b2",
    "b2 client",
    "r2",
    "r2 client",
    "cloudflare",
    "cloudflare r2",
    "AWS S3",
    "Azure Blob Storage",
    "Google Cloud Storage",
    "Ceph",
    "mibion",
    "backblaze"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nikeee/lean-s3"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "test": "tsgo && node --experimental-strip-types --test src/*.test.ts",
    "test:integration": "tsgo && node --experimental-strip-types --test src/test.integration.ts",
    "ci": "biome ci ./src",
    "docs": "typedoc",
    "lint": "biome lint ./src",
    "format": "biome format --write ./src && biome lint --write ./src && biome check --write ./src",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "fast-xml-parser": "^5.2.5",
    "undici": "^7.10.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@testcontainers/localstack": "^11.0.3",
    "@testcontainers/minio": "^11.0.3",
    "@types/node": "^24.0.1",
    "@typescript/native-preview": "^7.0.0-dev.20250613.1",
    "expect": "^30.0.0",
    "lefthook": "^1.11.13",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.5"
  },
  "engines": {
    "node": "^20.19.0 || ^22.14.0 || ^24.0.0"
  }
}
