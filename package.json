{
  "name": "lean-s3",
  "version": "0.9.12",
  "description": "A server-side S3 API for the regular user.",
  "keywords": [
    "AWS S3",
    "Azure Blob Storage",
    "Ceph",
    "Google Cloud Storage",
    "b2",
    "b2 client",
    "backblaze",
    "client",
    "cloudflare",
    "cloudflare r2",
    "mibion",
    "r2",
    "r2 client",
    "s3",
    "s3 client",
    "s3 sdk"
  ],
  "license": "MIT",
  "author": "Niklas Mollenhauer",
  "repository": {
    "type": "git",
    "url": "https://github.com/nikeee/lean-s3"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "scripts": {
    "build": "tsdown",
    "test": "tsgo && tsx --test src/*.test.ts src/test/*.test.ts",
    "test:integration": "tsgo && tsx --test src/test/test.integration.ts",
    "ci": "oxlint --type-aware --deny-warnings -f github",
    "docs": "typedoc",
    "format": "oxfmt",
    "lint": "oxlint --type-aware",
    "lint:fix": "oxlint --type-aware --fix",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "fast-xml-parser": "^5.3.7",
    "undici": "^7.22.0"
  },
  "devDependencies": {
    "@testcontainers/localstack": "^11.12.0",
    "@testcontainers/minio": "^11.12.0",
    "@testcontainers/s3mock": "^11.12.0",
    "@types/node": "^25.3.0",
    "@typescript/native-preview": "^7.0.0-dev.20260220.1",
    "expect": "^30.2.0",
    "lefthook": "^2.1.1",
    "oxfmt": "^0.34.0",
    "oxlint": "^1.49.0",
    "oxlint-tsgolint": "^0.14.2",
    "testcontainers": "^11.12.0",
    "tsdown": "^0.20.3",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.17"
  },
  "engines": {
    "node": "^20.19.5 || ^22.21.1 || ^24.11.0 || ^25.1.0"
  }
}
